            </Button>
            <Button fx:id="POOutBtn" mnemonicParsing="false" onAction="#Nav_DO_Out" prefHeight="25.0" prefWidth="285.0" style="-fx-background-color: #4B0082; -fx-background-radius: 1em;" text="Delivery Order (OUT)" textFill="WHITE">
